; This file was generated by chef for <%= @node[:fqdn] %>

; global
<% @stunnel[:global].each do |option,ary| -%>
<% ary = [ary].flatten -%>
<% ary.each do |v| -%>
<%= "#{option} = #{v}" %>
<% end -%>
<% end -%>

; services
<% @stunnel[:services].each do |name, config| -%>
<%= "[#{name}]" %>
<% config.each do |option,ary| -%>
<% ary = [ary].flatten -%>
<% ary.each do |v| -%>
<%= "#{option} = #{v}" %>
<% end -%>
<% end -%>
<% end -%>
